{% extends "layout.html.twig" %}

{% block title %}Home{% endblock %}

{% block content %}
    {% for billet in billets %}
        <article>
            <h2><a class="billetTitle" href="{{ path('billet', { 'billetId': billet.billetId }) }}">{{ billet.title }}</a></h2>
            <p>{{ billet.content }}</p>
        </article>
    {% endfor %}
{% endblock %}